{
  "version": 2,
  "builds": [
    {
      "src": "api/run.js",
      "use": "@vercel/node@4.2.4"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static@1.3.4"
    }
  ],
  "routes": [
    {
      "src": "/run",
      "dest": "/api/run",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
